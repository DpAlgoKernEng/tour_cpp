# 定义子模块（Submodule）的仓库路径和本地映射目录，通常由 git submodule add 命令自动生成
# git submodule update --init --recursive

[submodule "thirdparty/general_purpose/boost"]
	path = thirdparty/general_purpose/boost
	url = https://github.com/boostorg/boost.git

[submodule "thirdparty/cmdline_parser/gflags"]
	path = thirdparty/cmdline_parser/gflags
	url = https://github.com/gflags/gflags.git
